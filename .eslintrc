{
  "extends": [
    "prettier",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "env": {},
  "rules": {
    "strict": 2,
    "indent": ["error", 2, {"SwitchCase": 1}],
    "eqeqeq": 2,
    "no-console": 2,
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
